{
  "name": "bindicator-frontend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "prebuild": "npm rebuild vite && chmod +x ./node_modules/.bin/vite || echo 'skip chmod on Windows'",
    "build": "vite build",
    "preview": "vite preview --port 5173",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "framer-motion": "11.3.31"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "4.3.3",
    "autoprefixer": "10.4.20",
    "postcss": "8.4.47",
    "tailwindcss": "3.4.14",
    "vite": "5.4.8",
    "vitest": "1.6.0",
    "@testing-library/react": "14.2.1",
    "@testing-library/jest-dom": "6.4.8"
  },
  "vercel": {
    "cache": {
      "include": [
        "node_modules/.vite",
        "node_modules/.cache",
        "dist"
      ]
    }
  }
}
